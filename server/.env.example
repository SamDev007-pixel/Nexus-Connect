# Server Environment Variables
# Copy this to .env and update with your values

# Server Port
PORT=5001

# MongoDB Connection String
# Use MongoDB Atlas or local MongoDB
MONGODB_URI=mongodb://localhost:27017/aws-cloud-connect

# Or use MongoDB Atlas:
# MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/aws-cloud-connect?retryWrites=true&w=majority

# Node Environment
NODE_ENV=development
